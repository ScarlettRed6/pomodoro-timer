<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="oval">
            <solid android:color="?attr/iconSelectedColor1"/>
            <stroke android:width="2dp" android:color="?attr/iconSelectedColor2"/>
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="?attr/iconNotSelectedColor"/>
        </shape>
    </item>

</selector>